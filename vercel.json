{
  "functions": {
    "api/server.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/css/(.*)",
      "destination": "/public/css/$1"
    },
    {
      "source": "/js/(.*)",
      "destination": "/public/js/$1"
    },
    {
      "source": "/images/(.*)",
      "destination": "/public/images/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/api/server"
    }
  ]
}
